<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_margin="@dimen/small_size"
    android:padding="@dimen/small_size"
    android:background="@drawable/dashboard_screens_background"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/ll_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.033"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:contentDescription="@string/todo"
            android:src="@drawable/timesheet_icon" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:fontFamily="sans-serif-condensed-light"
            android:gravity="start"
            android:padding="@dimen/small_size"
            android:text="Time Sheet"
            android:textColor="@color/Blue_text_color"
            android:textSize="@dimen/thirty_dp" />


    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/ll_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:gravity="start"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ll_1"
        app:layout_constraintVertical_bias="0.033"
        tools:ignore="MissingConstraints">
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="center"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"

            tools:ignore="MissingConstraints">
            <TextView
                android:id="@+id/tv_total_team_members"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/left_space"
                android:gravity="center"
                android:fontFamily="@font/gill_sans"
                android:padding="@dimen/left_space"
                android:text="Total Team Members"
                android:textColor="@color/black"
                android:textSize="@dimen/Fifteen_dp" />
            <TextView
                android:id="@+id/tv_total_team_members_count"
                android:layout_width="wrap_content"
                android:layout_height="63dp"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/eight_dp"
                android:fontFamily="sans-serif-condensed-light"
                android:padding="@dimen/left_space"
                android:text="30"
                android:textColor="@color/green_count_color"
                android:textSize="@dimen/Fifty_dp">

            </TextView>


        </androidx.appcompat.widget.LinearLayoutCompat>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="start"
            android:layout_weight="1"
            android:orientation="vertical"
            tools:ignore="MissingConstraints">
            <TextView
                android:id="@+id/tv_not_submitted"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="@dimen/left_space"
                android:fontFamily="@font/gill_sans"
                android:padding="@dimen/left_space"
                android:text="Pending TimeSheets"
                android:textColor="@color/black"
                android:textSize="@dimen/Fifteen_dp" />
            <TextView
                android:id="@+id/tv_not_submitted_count"
                android:layout_width="wrap_content"
                android:layout_height="63dp"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/eight_dp"
                android:fontFamily="sans-serif-condensed-light"
                android:padding="@dimen/left_space"
                android:text="3"
                android:textColor="@color/green_count_color"
                android:textSize="@dimen/Fifty_dp">

            </TextView>


        </androidx.appcompat.widget.LinearLayoutCompat>
        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/ll_5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="start"
            android:layout_weight="1"
            android:orientation="vertical"

            tools:ignore="MissingConstraints">
            <TextView
                android:id="@+id/tv_submitted_members"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="@dimen/left_space"
                android:fontFamily="@font/gill_sans"
                android:padding="@dimen/left_space"
                android:text="Submitted TimeSheets"
                android:textColor="@color/black"
                android:textSize="@dimen/Fifteen_dp" />
            <TextView
                android:id="@+id/tv_submitted_members_count"
                android:layout_width="wrap_content"
                android:layout_height="63dp"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/eight_dp"
                android:fontFamily="sans-serif-condensed-light"
                android:padding="@dimen/left_space"
                android:text="27"
                android:textColor="@color/green_count_color"
                android:textSize="@dimen/Fifty_dp">

            </TextView>


        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>